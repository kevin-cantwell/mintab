{
    "manifest_version": 3,
    "name": "Tab Deduplicator",
    "version": "1.0",
    "description": "Closes duplicate tabs and switches to existing ones.",
    "permissions": ["tabs", "windows"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Tab Deduplicator"
    }
}
